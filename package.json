		"onCommand:adbInterface.adbResetPorts",
		"onCommand:adbInterface.disconnectEverthing"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "adbInterface.adbResetPorts",
				"title": "ðŸ“± ADB Interface :: Reset TCP ports from connected devices to :5555"
			},
			{
				"command": "adbInterface.adbwificonnect",
				"title": "ðŸ“± ADB Interface :: Connect to and device by IP over wifi"
			},
			{
				"command": "adbInterface.disconnectEverthing",
				"title": "ðŸ“± ADB Interface :: Disconnect from any devices"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.30"
	},
